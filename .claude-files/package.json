// Source: client/package.json
{
  "name": "fitness-timer",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "license": "0BSD",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "husky install",
    "android:dev": "expo start --android",
    "android:build": "expo build:android --release-channel production",
    "android:build:dev": "expo build:android --release-channel staging",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:debug": "cd android && ./gradlew assembleDebug",
    "ios:dev": "expo start --ios",
    "ios:build": "expo build:ios --release-channel production",
    "ios:build:dev": "expo build:ios --release-channel staging",
    "clean": "expo r -c",
    "doctor": "expo doctor"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "expo": "~52.0.27",
    "expo-av": "~15.0.2",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-sound": "^0.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^18.3.18",
    "babel-jest": "^29.2.1",
    "eslint": "^7.32.0",
    "eslint-config-react-native": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=16.5.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react-native",
    "expo",
    "fitness",
    "timer",
    "workout",
    "sports"
  ]
}
